

<%=render :layout => 'common/mobile',
          :locals => { :page_id => mobile_page_id() , :page_title => "Entrega de Material" } do %>
   
  <div role="main" class="ui-content">   

<table class="tabla_campos_lectura" cellpadding=0 cellspacing=0>
  <tr>   
  <th>Material:</th>  
	<td> <%= @shipment_supply.requested_supply.requested_cargo.description %> </td>
  </tr>
  <tr>
   <th>Cantidad:</th>  
	<td> <%= @shipment_supply.amount %> <%= shipment_supply.requested_supply.requested_cargo.measure_unit.symbol %>  </td>
  </tr>
  
    <div class="links_form"><%= link_to 'Salir',  shipments_path(@shipment)  , {"data-ajax"=>"false"}%></div> 

  </div> <!-- /content --> 
<% end %>
