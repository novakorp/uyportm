
<%=render :layout => 'common/mobile',
          :locals => { :page_id => mobile_page_id() , :page_title => "Información de Vehículos de GPS" } do %> 

  <div role="main" class="ui-content">  


	 <%= link_to 'Volver', "/welcome/configuration" %> <br/>

	Total Vehículos: <%= @info.length %> <br/>

	<table class="tabla_listado" cellpadding=0 cellspacing=0>
	  <tr class="tabla_listado_fila_cabezal">
	
			<th colspan="5" style="border-width:0 2px 0 0 ; border-color:white ; border-style:solid">Sistema</th>
			<th colspan="3">GPS</th>
		</tr>
	 <tr class="tabla_listado_fila_cabezal">
	
			<th>id</th>
			<th>patente</th> 
			<th>gps_vehicle_id</th>
			<th>gps_plate_number</th>
			<th style="border-width:0 2px 0 0 ; border-color:white ; border-style:solid">gps_description</th>
			<th>patente</th>	 
			<th>movil id</th>	  		
			<th>identificador</th>	
		</tr>
	
		<% @info.each do |data| %>
			<% if ! data.nil? %>
			<tr>
				<% v = data[:vehicle] %>
			
				<% if ! v.nil? %> 
					<td><%= v.id %></td>
				 	<td><%= v.plate_number %></td>
				 	<td><%= v.gps_vehicle_id %></td>			 	
				 	<td><%= v.gps_plate_number %></td>
				 	<td><%= v.gps_description %></td>
				<% else %>
				 	<td>**</td>
				 	<td>**</td>
				 	<td>**</td>
				 	<td>**</td>
				 	<td>**</td>
				<% end %>
			
		    	 <td><%=  data[:movil_id]  %> </td>
				 <td><%=  data[:patente]  %> </td> 
		      	 <td><%=  data[:identificador]  %> </td>
		      	
			</tr>	
		
			<% end %>
		<% end %>
		
	</table>


	<div class="links_listado"><%= link_to 'Volver', "/welcome/configuration" %></div>

  </div> <!-- /content --> 
  
<% end %>
	 

	 
