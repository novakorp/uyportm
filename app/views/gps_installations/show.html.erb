

<%=render :layout => 'common/mobile',
          :locals => { :page_id => mobile_page_id() , :page_title => "Dispositivo GPS" } do %>
   
  <div role="main" class="ui-content">   

	<table class="tabla_campos_lectura" cellpadding=0 cellspacing=0>
	   <tr>
	  <th>Ident Numerico GPS</th>
	  <td><%= @gps_installation.gps_numeric_ident %></td>
	  </tr>
	  <tr>
	  <th>Ident Descriptivo GPS:</th>
	  <td><%= @gps_installation.gps_descriptive_ident  %></td>
	  </tr>
	  <tr>
	  <th>Matrícula GPS:</th>
	  <td><%= @gps_installation.gps_plate_number %></td>
