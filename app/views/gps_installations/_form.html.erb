  
<%= form_for @gps_installation do |f| %>
 <% if @gps_installation.errors.any? %>
  <div id="error_explanation" class="form_errors">
  <h2> <%= t :error_header %> </h2>
    <ul>
    <% @gps_installation.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
  
  

  
  <table class="tabla_campos" cellpadding=0 cellspacing=0>
  
  <tr>
  <th><%= f.label :gps_numeric_ident %></th>
  <td> <%= f.text_field :gps_numeric_ident %> </td>
  </tr>
  
  <tr>
  <th><%= f.label :gps_descriptive_ident %></th>
  <td> <%= f.text_field :gps_descriptive_ident %> </td>
  </tr>
    
  <tr>
  <th><%= f.label :gps_plate_number %></th>
  <td> <%= f.text_field :gps_plate_number %> </td>
  </tr>
     
  <tr>
  <th><%= f.label :vehicle_id %></th>
  <td> <%= f.text_field :vehicle_id %> </td>
  </tr>
  
  <tr><td colspan=2 class="acciones_form">
		<%= f.submit t(:save) %>
  </td>
  </tr>
  </table>
  
<% end %>


