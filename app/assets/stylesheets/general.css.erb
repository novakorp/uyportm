
/* Pie de pagina general */

.footer_odacor { 
  font-weight: normal;
  font-size: 5px;
  color: green;
}

/* para autocomplete */
.ui-filter-inset {
    margin-top: 0;
}


.tabla_principal {width: 100%; height:700px;}
.tabla_principal_encabezado {
	background-image: url('<%= asset_path 'fondo_banner_der.jpg' %>') ;
	background-repeat: repeat-x;
	height: 45px;
}

.tabla_principal_banner {
	
  width: 133px;
	height: 45px;
}

.tabla_principal_borde_encabezado {
	border-width: 0;
	border-style: solid;
	border-color: #1F9625;
	height: 0px; 
}

.tabla_principal_contenido {
	border-width: 0px;
  border-style: solid;
   border-color:  #009902; 
  /*  border-color:  #24AF2E; */
   /*border-color:  #303030;*/
	padding: 5px 15px 15px 10px; 
	vertical-align: top; 
	background-color: #FEFEFE;
	background-image: url(<%= asset_path 'fondo_contenido_disenio_gral.jpg' %>) ;
	background-repeat: no-repeat;
	background-position: right top;
	} 
	
.titulo_sistema {
	color: white;
	font-size: 16pt;
	font-weight: bold;
}

.menu_superior {
  margin-left : 10px;
  margin-bottom : 20px;
}

